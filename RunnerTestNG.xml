<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="SearchVehicle" thread-count="1" data-provider-thread-count="1">
    <parameter name="browser" value="CHROME"/>
    <parameter name="environment" value="PROD"/>
    <parameter name="quit" value="YES"/>

    <test name="Search old and new automobile">
        <classes>
            <class name="tests.Automobile"/>
        </classes>
    </test>

    <parameter name="browser" value="CHROME"/>
    <parameter name="environment" value="PROD"/>
    <parameter name="quit" value="YES"/>

    <test name="Search old and new motorcycles">
        <classes>
            <class name="tests.Motorcycles"/>
        </classes>
    </test>

    <parameter name="browser" value="CHROME"/>
    <parameter name="environment" value="PROD"/>
    <parameter name="quit" value="YES"/>

    <test name="Search old and new bus">
        <classes>
            <class name="tests.TransportVehiclesBus"/>
        </classes>
    </test>
</suite>



